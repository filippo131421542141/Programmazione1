{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["-input ciao ciao.txt"],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/workspaces/EsercitazioniLab",
      "program": "/workspaces/EsercitazioniLab/build/Debug/outDebug ",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      
    }
  ]
}